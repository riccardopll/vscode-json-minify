{
  "name": "rpll-json-tools",
  "displayName": "JSON Tools X",
  "version": "0.0.1",
  "description": "A VS Code extension that provides JSON manipulation commands.",
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "json",
    "minify",
    "formatter",
    "compress",
    "stringify",
    "deserialize",
    "parse"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/riccardopll/vscode-json-minify.git"
  },
  "license": "MIT",
  "author": "Riccardo Palleschi",
  "publisher": "riccardopll",
  "main": "./dist/extension.js",
  "scripts": {
    "check-types": "tsc --noEmit",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "compile-tests": "tsc -p . --outDir out && cp -r src/test/resources out/test/",
    "lint": "eslint src --fix && prettier --write .",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "test": "vscode-test",
    "vsce:package": "vsce package",
    "vsce:publish": "vsce publish",
    "vscode:prepublish": "npm run package",
    "watch": "npm-run-all -p watch:*",
    "watch-tests": "tsc -p . -w --outDir out & cp -r src/test/resources out/test/",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json"
  },
  "contributes": {
    "commands": [
      {
        "command": "json-tools.minify",
        "title": "JSON: Minify"
      },
      {
        "command": "json-tools.stringify",
        "title": "JSON: Stringify"
      },
      {
        "command": "json-tools.deserialize",
        "title": "JSON: Deserialize"
      }
    ]
  },
  "activationEvents": [],
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "npm-check-updates": "^18.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "vscode": "^1.102.0"
  }
}
